<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="StartScenePane" prefHeight="500.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.ap_proj.Controller">
   <children>
      <ImageView fitHeight="600.0" fitWidth="600.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@projimg.png" />
         </image>
      </ImageView>
      <Button layoutX="225.0" layoutY="312.0" mnemonicParsing="false" onAction="#switchToGameScene" prefWidth="150.0" text="START GAME" textAlignment="CENTER" wrapText="true">
         <font>
            <Font name="Arial Black" size="16.0" />
         </font></Button>
      <Label layoutX="97.0" layoutY="90.0" prefWidth="408.0" text="STICK HERO " textFill="#f4f4f8">
         <font>
            <Font name="Arial Black" size="56.0" />
         </font>
      </Label>
      <Button fx:id="ExitButton" layoutX="265.0" layoutY="396.0" mnemonicParsing="false" onAction="#exit" prefWidth="70.0" text="EXIT">
         <font>
            <Font name="Arial Black" size="16.0" />
         </font>
      </Button>
      <Button layoutX="250.0" layoutY="270.0" mnemonicParsing="false" onAction="#switchToGameScene" prefWidth="100.0" text="Continue" textAlignment="CENTER" wrapText="true">
         <font>
            <Font name="Arial Black" size="16.0" />
         </font>
      </Button>
      <Button fx:id="ExitButton1" layoutX="263.0" layoutY="354.0" mnemonicParsing="false" onAction="#exit" prefWidth="74.0" text="SHOP">
         <font>
            <Font name="Arial Black" size="16.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
